<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <include
        android:id="@+id/toolbar_register_template"
        layout="@layout/toolbar_register">
    </include>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView5" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.newnergy.para_client.CircleImageView
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:id="@+id/imageView_furtherInfo_profilePicture"
                android:layout_gravity="center"
                android:layout_marginTop="25dp"
                android:src="@drawable/client_photo_round" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView_furtherInfo_name"
                android:layout_gravity="center"
                android:layout_marginTop="12.5dp"
                android:gravity="center_vertical|center_horizontal"
                android:textColor="#000000"
                android:textSize="20sp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:id="@+id/bottons"
                android:layout_height="75dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/buttons_container">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageButton_location_furtherInfo"
                        android:src="@drawable/button_location_xh_720x1280"
                        android:layout_weight="1"
                        android:paddingTop="20dp"
                        android:paddingBottom="20dp"
                        android:paddingLeft="50dp" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageButton_furtherInfo_dial"
                        android:src="@drawable/button_call_xh_720x1280"
                        android:layout_weight="1"
                        android:paddingBottom="20dp"
                        android:paddingTop="20dp" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageButton_chat_furtherInfo"
                        android:src="@drawable/button_chat_xh_720x1280"
                        android:layout_weight="1"
                        android:paddingTop="20dp"
                        android:paddingBottom="20dp"
                        android:paddingRight="50dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="12.5dp" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Rating"
                        android:id="@+id/textView118"
                        android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_weight="1"
                        android:layout_marginLeft="12.5dp"
                        android:gravity="center_vertical" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout_starContainer"
                        android:layout_marginLeft="12.5dp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/median_full_star"
                            android:id="@+id/imageView_star1"
                            android:layout_weight="1"
                            android:src="@drawable/median_full_star" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/median_full_star"
                            android:id="@+id/imageView_star2"
                            android:layout_weight="1" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/median_full_star"
                            android:id="@+id/imageView_star3"
                            android:layout_weight="1" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/median_full_star"
                            android:id="@+id/imageView_star4"
                            android:layout_weight="1" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/median_full_star"
                            android:id="@+id/imageView_star5"
                            android:layout_weight="1" />
                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView55"
                    android:src="@drawable/divide_line_furtherinfo_xh_720x1280"
                    android:layout_marginTop="12.5dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/detail">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="12.5dp"
                    android:id="@+id/phone">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Phone"
                            android:id="@+id/textView120"
                            android:textSize="20sp"
                            android:textColor="#000000"
                            android:layout_weight="1"
                            android:layout_marginLeft="12.5dp"
                            android:gravity="center_vertical" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView_furtherInfo_phoneNo"
                            android:layout_weight="1"
                            android:textSize="20sp"
                            android:textColor="#999999"
                            android:gravity="right"
                            android:layout_marginRight="12.5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView56"
                        android:src="@drawable/divide_line_furtherinfo_xh_720x1280"
                        android:layout_marginTop="12.5dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="12.5dp"
                    android:id="@+id/email">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Email"
                            android:id="@+id/textView129"
                            android:textSize="20sp"
                            android:textColor="#000000"
                            android:layout_weight="1"
                            android:layout_marginLeft="12.5dp"
                            android:gravity="center_vertical" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView_furtherInfo_email"
                            android:layout_weight="1"
                            android:textSize="20sp"
                            android:textColor="#999999"
                            android:gravity="right"
                            android:layout_marginRight="12.5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView58"
                        android:src="@drawable/divide_line_furtherinfo_xh_720x1280"
                        android:layout_marginTop="12.5dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="12.5dp"
                    android:id="@+id/address">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Address"
                            android:id="@+id/textView117"
                            android:textSize="20sp"
                            android:textColor="#000000"
                            android:layout_weight="1"
                            android:layout_marginLeft="12.5dp"
                            android:gravity="center_vertical" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView_furtherInfo_address"
                            android:layout_weight="1"
                            android:textSize="20sp"
                            android:textColor="#999999"
                            android:gravity="right"
                            android:layout_marginRight="12.5dp" />

                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView54"
                        android:src="@drawable/divide_line_furtherinfo_xh_720x1280"
                        android:layout_marginTop="12.5dp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Skills"
                android:id="@+id/textView84"
                android:textSize="20sp"
                android:textColor="#000000"
                android:layout_weight="1"
                android:layout_marginLeft="25dp"
                android:gravity="center_vertical" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView_skills"
                android:layout_marginTop="12.5dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginBottom="12.5dp"
                android:textColor="#999999"
                android:textSize="20sp" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageView41"
                android:src="@drawable/divide_line_furtherinfo_xh_720x1280"
                android:layout_marginTop="12.5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Offer task"
                android:id="@+id/button_furtherInfo_offerJob"
                android:layout_gravity="center"
                android:textColor="#ffffff"
                android:textSize="18sp"
                android:textStyle="bold"
                android:background="@drawable/border_radius_blue_button"
                android:layout_marginTop="12.5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="40dp"
                android:paddingRight="40dp" />

            <android.support.v4.view.ViewPager
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:id="@+id/viewpager_furtherinfo"
                android:layout_gravity="center_horizontal"
                android:layout_margin="12.5dp" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>